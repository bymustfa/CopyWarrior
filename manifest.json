{
  "manifest_version": 2,
  "name": "Copy Warrior",
  "description": "CopyWarrior gelişmiş bir kopyala / yapıştır aracıdır.",
  "version": "1.1",
  "permissions": [
    "contextMenus",
    "tabs",
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "background": {
    "scripts": [
      "./assets/js/jquery-3.5.0.min.js",
      "./copywarrior.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "./assets/icons/16x.png",
    "24": "./assets/icons/24x.png",
    "32": "./assets/icons/32x.png",
    "64": "./assets/icons/64x.png",
    "128": "./assets/icons/128x.png",
    "256": "./assets/icons/256x.png"
  },
  "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'"
}